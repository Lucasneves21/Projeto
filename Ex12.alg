algoritmo "semnome"
// Função :
// Autor :
// Data : 15/02/2022
// Seção de Declarações 
var
   ano, nasc, idade : inteiro
inicio
      Escreval ("Qual ano voce nasceu ")
      leia (nasc)
      Escreval ("Em que ano nos estamos ")
      leia (ano)
      idade <- ano - nasc
      Escreval ("Você tem ",idade, " anos")
      Escreva ("Daqui a 17 anos voce tera", idade + 17)
fimalgoritmo
