algoritmo "semnome"
// Função :
// Autor :
// Data : 02/02/2022
// Seção de Declarações 
var
   Valor, Juros:Real
   
inicio
      EscrevaL ("Digite o valor para depositarmos em sua poupanca ")
      leia (Valor)
      Juros <- (Valor * 1.3)/100
      Escreval ("Voce recebera esse mes",Juros, " Reais de juros")
fimalgoritmo
